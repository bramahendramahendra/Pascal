Program TDasar;
Uses crt;
Var
        a:word;
        i:integer;
        z:longint;
        x,y:real;
        found:boolean;

Begin
        a:=15;
        writeln('a = ',a);
        i:=15000;
        writeln('i = ',i);
        x:=0.5;
        writeln('x = ',x);
        y:=1.56789;
        writeln('y = ',y);
        z:=1000000;
        writeln('z = ',z);
        found:=true;
        writeln('found = ',found);
        readln;
End.